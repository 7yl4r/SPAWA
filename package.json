        "fastclick": "http://cdnjs.cloudflare.com/ajax/libs/fastclick/0.6.7/fastclick.min.js",
        "ui.bootstrap": "http://cdnjs.cloudflare.com/ajax/libs/angular-ui-bootstrap/0.12.1/ui-bootstrap.min.js",
        "ngTouch": "http://cdnjs.cloudflare.com/ajax/libs/angular.js/1.3.14/angular-touch.min.js",
        "header-navbar": "navHeader/navHeader.js",
        "splash-header": "splashHeader/splashHeader.js",
        "app-footer": "appFooter/appFooter.js"
    }
}